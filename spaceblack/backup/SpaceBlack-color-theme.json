	"tokenColors": [
		{
			"settings": {
				"foreground": "#c0c5ce"
			}
		},
		{
			"name": "Text",
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#c0c5ce"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#65737e"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.string",
				"punctuation.definition.variable",
				"punctuation.definition.string",
				"punctuation.definition.parameters",
				"punctuation.definition.string",
				"punctuation.definition.array"
			],
			"settings": {
				"foreground": "#c0c5ce"
			}
		},
		{
			"name": "Delimiters",
			"scope": "none",
			"settings": {
				"foreground": "#c0c5ce"
			}
		},
		{
